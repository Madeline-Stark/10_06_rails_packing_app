<%= form_for(@list) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <br>
    <%= f.fields_for :items do |i| %>
        <%= i.label :name %>
        <%= i.text_field :name %>

        <%= i.label :quantity %>
        <%= i.text_field :quantity %>
        <br>
    <% end %>

    <%= f.submit %>
<% end %>